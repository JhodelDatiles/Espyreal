{
  "expo": {
    "name": "Espyreal",
    "slug": "Espyreal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Espyreal-icon.png",
    "scheme": "espyreal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": { 
        "foregroundImage": "./assets/images/Espyreal-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Espyreal"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Espyreal-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Espyreal-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ba30d479-9bcd-4c8e-80e3-97bead109bd9"
      }
    }
  }
}
